


<div class="section-big">
  <div class="container">
    <div class="col-md-12 col-sm-12 col-xs-12>

      <div class="ibox-content">
        <%= render 'notices' %>
        <div class="center_div">
          <div class="row">
            <h3 class="table-labels">Tags</h3>

          </div>
          <div class="row-table ">
            <div class="table-responsive">
              <table data-toggle="table" data-pagination="true"  class="table table-striped table-bordered"'>
                <thead class="thead-light">
                <tr>
                  <th data-sortable="true"> Last Name</th>
                  <th data-sortable="true"> First Name</th>
                  <th data-sortable="true"> New Tag</th>
                </tr>
              </thead>
              <tbody>
                <% @hello.each do |participant| %>
                  <tr>
                    <td>
                      <%= participant.flatten(2)[0] %>
                    </td>
                    <td>
                      <%= participant.flatten(2)[1] %>
                    </td>
                    <td>
                      <%= participant.flatten(2).last %>
                    </td>
                  </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
